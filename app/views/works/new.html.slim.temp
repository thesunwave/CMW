= render partial: "components/errors"
= form_for @work, url: { action: "create" }, multipart: true do |f|
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :description
    = f.text_field :description
  .field
    = f.label :tags
    = f.text_field :tags
  .field
    = f.label :image
    = f.file_field :image
  .actions
    = f.submit 'Upload you work'
    = link_to 'Go back', list_path, class: 'button'